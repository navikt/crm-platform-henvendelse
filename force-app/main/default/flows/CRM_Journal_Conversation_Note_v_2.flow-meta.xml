    <formulas>
        <name>Document_Title</name>
        <dataType>String</dataType>
        <expression>&apos;Samtale med NAV - &apos; +
            LPAD(TEXT(DAY(DATEVALUE({!JOURNALED_CONV_NOTE.CRM_Registered_Datetime_Formula__c}))),2,&apos;0&apos;)
            + &apos;.&apos; +
            LPAD(TEXT(MONTH(DATEVALUE({!JOURNALED_CONV_NOTE.CRM_Registered_Datetime_Formula__c}))),2,&apos;0&apos;)
            + &apos;.&apos; +
            TEXT(YEAR(DATEVALUE({!JOURNALED_CONV_NOTE.CRM_Registered_Datetime_Formula__c}))) +
            &apos; - &apos; +
            {!JOURNALED_CONV_NOTE.CRM_Theme_Name__c}</expression>
    </formulas>
    <formulas>
        <name>externalReference</name>
        <dataType>String</dataType>
        <expression>{!JOURNALED_CONV_NOTE.Id} + {!Journal_Theme_Code} +
            SUBSTITUTE(URLENCODE({!Journal_Sak.fagsakId}), &apos;%&apos;, &apos;_&apos;)</expression>
    </formulas>
    <formulas>
        <description>Better null/blank handling for passing this value to apex</description>
        <name>Journal_Case_Legacy_System_Formula</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Journal_Case_Legacy_System}), null, {!Journal_Case_Legacy_System})</expression>
    </formulas>
    <formulas>
        <description>Formula to define the journal case type (Sakstype). If no Journal Case ID has
            been input, default is GENERELL_SAK</description>
        <name>Journal_Case_Type</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Journal_Case_ID}), &apos;GENERELL_SAK&apos;, &apos;FAGSAK&apos;)</expression>
    </formulas>
    <formulas>
        <description>Formula to generate name convention on journal required format (For persons)</description>
        <name>Person_Full_Name</name>
        <dataType>String</dataType>
        <expression>{!Get_Account_Info.CRM_Person__r.INT_LastName__c} &amp; &apos;, &apos; &amp;
            {!Get_Account_Info.CRM_Person__r.INT_FirstName__c} &amp; &apos; &apos;
            &amp;{!Get_Account_Info.CRM_Person__r.INT_MiddleName__c}</expression>
    </formulas>
